function preexec() {
    command="${1%% *}"
}

function precmd() {
    (($?)) && [ -n "$command" ] && [ -x /usr/bin/python ] && [ -x /usr/bin/command-not-found ] && {
        whence -- "$command" >& /dev/null ||
            /usr/bin/python /usr/bin/command-not-found "$command" zypp
        unset command
    }
}
